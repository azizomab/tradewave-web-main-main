{% extends 'admin.base.html.twig' %}

{% block title %}Market{% endblock %}

{% block body %}
    <h1>Market</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IDmarket</th>
                <td>{{ market.IDmarket }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ market.name }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ market.rate }}</td>
            </tr>
            <tr>
                <th>Sprice</th>
                <td>{{ market.Sprice }}</td>
            </tr>
            <tr>
                <th>Bprice</th>
                <td>{{ market.Bprice }}</td>
            </tr>
            <tr>
                <th>Mcap</th>
                <td>{{ market.Mcap }}</td>
            </tr>
            <tr>
                <th>TypeM</th>
                <td>{{ market.typeM }}</td>
            </tr>
            <tr>
                <th>IMG_SRC</th>
                <td>{{ market.IMGSRC }}</td>
            </tr>
            <tr>
                <th>ID_user</th>
                <td>{{ market.IDUser }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_market_index') }}">back to list</a>

    <a href="{{ path('app_market_edit', {'id': market.IDmarket}) }}">edit</a>

    {{ include('Back/market/_delete_form.html.twig') }}
    
{% endblock %}
