<script>
    $(document).ready(function() {
        $('#search-form').submit(function(event) {
            event.preventDefault(); // Empêche le formulaire de se soumettre normalement

            var searchTerm = $('#search-input').val(); // Récupère la valeur du champ de recherche
            window.location.href = "{{ path('user_list') }}?search=" + searchTerm; // Redirige vers la page de liste des utilisateurs avec le terme de recherche dans l'URL
        });
    });
</script>
